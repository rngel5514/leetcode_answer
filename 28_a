class Solution(object):
    def strStr(self, haystack, needle):
        """
        :type haystack: str
        :type needle: str
        :rtype: int
        """
        """
        Return the index of the first 
        occurrence of needle in haystack, 
        or -1 if needle is not part of
        haystack.
        """
        str_index = haystack.find(needle,0)
        return str_index
